    <script>

        const words = ["init() IT Association"];
        let i = 0;
        let j = 0;
        let currentWord = "";
        let isDeleting = false;
        let typingDone = false; // To check when typing is complete

        function type() {
            currentWord = words[i];

            if (isDeleting) {
                document.getElementById("typewriter").textContent = currentWord.substring(0, j - 1);
                j--;
                if (j == 0) {
                    isDeleting = false;
                    i++;
                    if (i == words.length) {
                        i = 0;
                    }
                }
            } else {
                document.getElementById("typewriter").textContent = currentWord.substring(0, j + 1);
                if (j == currentWord.length) {

                    typingDone = true; // Mark typing as complete when the word is fully typed
                    showElementsAfterTyping(); // Call this function to fade in elements
                }
                j++;
            }
            setTimeout(type, 120); // Continue typing if not done
        }

        // Function to fade in elements after typing is complete
        function showElementsAfterTyping() {
            setTimeout(() => {
                document.getElementById("empowering-text").classList.add('visible-empowering'); // Show the text
                document.getElementById("dive-button").classList.add('visible-empowering');     // Show the button
            }, 500); // Adjust the delay if needed
        }

        window.onload = function () {
            type();
        };


        // JavaScript for toggling mobile navigation
        const hamburger = document.getElementById('hamburger');
        const mobileNav = document.getElementById('mobileNav');
        const closeMobileNav = document.getElementById('closeMobileNav');

        hamburger.addEventListener('click', () => {
            mobileNav.style.transform = 'translateX(0)';
        });

        closeMobileNav.addEventListener('click', () => {
            mobileNav.style.transform = 'translateX(-100%)';
        });

        // Scroll animation
        const faders = document.querySelectorAll('.fade-in');
        const appearOptions = {
            threshold: 0.4,
            rootMargin: '0px 0px -50px 0px'
        };

        const appearOnScroll = new IntersectionObserver(function (entries, appearOnScroll) {
            entries.forEach(entry => {
                if (!entry.isIntersecting) {
                    return;
                } else {
                    entry.target.classList.add('visible');
                    appearOnScroll.unobserve(entry.target);
                }
            });
        }, appearOptions);

        faders.forEach(fader => {
            appearOnScroll.observe(fader);
        });
        const navbar = document.querySelector('nav');
        window.addEventListener('scroll', () => {
            if (window.scrollY > 50) {
                navbar.classList.add('scrolled');
            } else {
                navbar.classList.remove('scrolled');
            }
        });
    </script>
