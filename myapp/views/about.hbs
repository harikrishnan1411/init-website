        /* General Styles */
        html {
            color-scheme: dark;
            /* or use 'dark' if you want your website to always be dark themed */
        }

        nav {
            font-family: "Kode Mono", monospace;
            font-optical-sizing: auto;
            font-weight: <weight>;
            font-style: normal;
        }

        main {
            display: flex;
            align-items: center;
            justify-content: center;
            margin-top: 20px;
        }

        section {
            padding: 50px 20px;
        }

        /* Fade-in animation */
        .fade-in {
            opacity: 0;
            transform: translateY(20px);
            transition: opacity 0.6s ease-out, transform 0.6s ease-out;
        }

        .fade-in.visible {
            opacity: 1;
            transform: translateY(0);
        }

        /* Mobile Nav */


        /* Ensure mobile nav links are styled and visible */
        #mobileNav {
            transform: translateX(100%);
            /* Start off-screen to the right */
            transition: transform 0.3s ease;
            top: 0;
            /* Aligns with the top of the viewport */
            right: 0;
            /* Aligns with the right side of the viewport */
            height: 100vh;
            /* Ensures it takes the full height of the viewport */
            width: 100%;
            /* Adjust the width as needed */
            padding: 0;
            /* Remove any default padding */
            margin: 0;
            /* Remove any default margin */
            background-color: rgba(0, 0, 0, 0.8);
            /* Semi-transparent background */
            display: flex;
            flex-direction: column;
            align-items: center;
            font-size: 1.5rem;
            z-index: 9999;
            transform: translateX(-100%);
            transition: transform 0.3s ease;
            top: 0;
            /* Ensure it is aligned with the top */
            left: 0;
        }

        #mobileNav a:hover {
            background-color: rgba(255, 255, 255, 0.2);
        }

        #mobileNav.active {
            transform: translateX(0);
        }

        #mobileNav {
            transform: translateX(-100%);
            transition: transform 0.3s ease;
            top: 0;
            /* Ensure it is aligned with the top */
            left: 0;
            /* Align with the left side */
        }

        /* Mobile Nav active state */
        #mobileNav.active {
            transform: translateX(0);
        }

        #closeMobileNav {
            margin: 1rem;
            /* Adjust margin if necessary */
            align-self: flex-end;
            /* Align to the end of the container */
        }

        /* Navbar scroll effect */
        nav.scrolled {
            background-color: rgba(0, 0, 0, 0.8);
            transition: background-color 0.3s ease;
        }

        /* Section Styles */
        .section-title {
            text-align: center;
            font-size: 2.5rem;
            font-weight: bold;
            margin-bottom: 30px;
            color: #333;
        }

        .section-title span {
            color: #6366F1;
            border-bottom: 2px solid #6366F1;
            padding-bottom: 5px;
        }

        .section-description {
            max-width: 800px;
            margin: 0 auto;
            text-align: center;
            color: #555;
            margin-bottom: 40px;
        }

        .grid-3 {
            display: grid;
            grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
            gap: 40px;
            align-items: center;
        }

        .card {
            background-color: #fff;
            padding: 20px;
            border-radius: 12px;
            box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease;
        }

        .card:hover {
            transform: translateY(-10px);
        }

        .card img {
            width: 100%;
            border-radius: 10px;
            margin-bottom: 20px;
        }

        .card h3 {
            font-size: 1.5rem;
            margin-bottom: 10px;
        }

        .card p {
            color: #666;
        }

        /* Call to Action Section */
        .cta {
            background-color: #6366F1;
            color: white;
            padding: 60px 30px;
            text-align: center;
            border-radius: 10px;
            margin: 40px 0;
        }

        .cta h2 {
            font-size: 2.5rem;
            margin-bottom: 20px;
        }

        .cta p {
            font-size: 1.2rem;
            margin-bottom: 30px;
        }

        .cta button {
            background-color: white;
            color: #6366F1;
            padding: 10px 20px;
            border-radius: 30px;
            font-weight: bold;
            font-size: 1.1rem;
            transition: background-color 0.3s ease;
        }

        .cta button:hover {
            background-color: #E0E7FF;
        }

        @media (prefers-color-scheme: light) {
            html {
                color-scheme: dark;
                /* Force dark mode */
            }
        }
    </style>
